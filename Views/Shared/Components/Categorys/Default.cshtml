@{
}
@model IEnumerable<string>

<div class="d=grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-bookCat="" class="btn btn-outline-secondary">Home</a>

    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-bookCat="@t" class="btn btn-outline-secondary">@t</a>
    }

</div>